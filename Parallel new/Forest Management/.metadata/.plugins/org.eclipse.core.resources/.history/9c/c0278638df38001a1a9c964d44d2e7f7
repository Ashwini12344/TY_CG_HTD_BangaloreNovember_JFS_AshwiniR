addContractQuery=insert into contract values(?,?,?,?,?,?)
deleteContractQuery=delete from contract where contractNo=? 
addCustomerQuery=insert into customer_info values(?,?,?,?,?,?,?,?)
deleteCustomerQuery=delete from customer where customerid=? 
modifyCustomerQuery=update customer set customerName=?,streetAddress1=?,streetAddress2=?,town=?,postalcode=?,email=? where customerid=?
addProductQuery=insert into product values(?,?,?,?)
deleteProductQuery=delete from product where prodId=? 
addLandQuery=insert into land values(?,?,?,?)
deleteLandQuery=delete from land where landId=? 
addSchedule=insert into schedule_info values(?,?,?,?,?)
deleteSchedule=delete from schedule_info where scheduleId = ? 
getAllSchedule=select* from schedule_info
getSchedule=select * from schedule_info where scheduleId = ? 
modifySchedule= update schedule_info set contractId=?,productId =?,quantity=?,deliveryDate=? where scheduleId = ?

